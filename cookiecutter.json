{
  "project_name": "My New Project",
  "year": "{% now 'local', '%Y' %}",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "description": "",
  "license": "University of Illinois/NCSA Open Source License",
  "version": "0.0.1",
  "url": "http://www.library.illinois.edu/dccdocs/{{cookiecutter.project_slug}}",
  "source_control": "https://github.com/UIUCLibrary/{{cookiecutter.project_slug}}",
  "maintainer": "Henry Borchers",
  "maintainer_email": "hborcher@illinois.edu",
  "author": "University Library at The University of Illinois at Urbana Champaign: Preservation Services",
  "author_email": "prescons@library.illinois.edu",
  "use_pytest": "y",
  "use_tox": "y",
  "use_sphinx": "y",
  "use_travis_ci": "y",
  "use_jenkins_pipeline": "y",
  "create_deployment_yml": "y",
  "script": {
    "add_script": "y",
    "cli_command_name": "{{ cookiecutter.project_slug }}"
  },
  "use_cx_freeze":"n",
  "create_standalone": "n",
  "_extensions": [
    "jinja2_time.TimeExtension"
  ]
}
